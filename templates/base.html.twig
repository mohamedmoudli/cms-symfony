        <script>
            // Initialize Clipboard.js
            var clipboard = new ClipboardJS('.copy-button', {
                text: function (trigger) {
                    var variableName = trigger.getAttribute('data-variable-target');
                    return variableName;
                }
            });

            // Handle success event
            clipboard.on('success', function (e) {
                $.toast({
                    heading: '',
                    text: 'Copié dans le presse-papiers',
                    position: 'top-right',
                    loaderBg: '#ff6849',
                    icon: 'success',
                    hideAfter: 1500,
                    stack: 6
                });
            });

            // Handle error event
            clipboard.on('error', function (e) {
                console.error('Unable to copy to clipboard:', e.action);
            });
